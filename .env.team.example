# Web UI Configuration
# Merged configuration for both individual and team modes

# Server Configuration
WEBUI_SERVER_PORT=8002
WEBUI_CLIENT_PORT=8003
SERVICE_BACKEND_PORT=8000
NODE_ENV=development
BASE_URL=http://localhost:8002

# ACP Configuration
ACP_ENABLED=true
ACP_SERVER_HOST=localhost
ACP_SERVER_PORT=8001
ACP_WEBSOCKET_URL=ws://localhost:8001
ACP_AGENT_CONFIG_PATH=./config/agents.json
ACP_DISCOVERY_MODE=configuration
ACP_HEALTH_CHECK_INTERVAL=30000

# Docker Sandbox Configuration
SANDBOX_ENABLED=true
SANDBOX_IMAGE=node:20-bookworm
SANDBOX_NETWORK=bridge
SANDBOX_MEMORY=1g
SANDBOX_CPUS=2
SANDBOX_IDLE_TIMEOUT=3600000
SANDBOX_CLEANUP_INTERVAL=300000

# Session Configuration
SESSION_TOKEN_LIMIT=128000
MESSAGE_WINDOW_SIZE=100

# Database Configuration (MongoDB-only with ACP)
MONGODB_URI=mongodb://your-user:your-password@localhost:27017/qwen_code?authSource=admin
MONGO_DATABASE=qwen_code

# NFS Storage
NFS_BASE_PATH=../../infrastructure/nfs-data

# JWT Configuration
JWT_SECRET=your-jwt-secret-change-in-production
JWT_EXPIRES_IN=24h

# OpenAI-compatible API
OPENAI_BASE_URL=your-api-base-url
OPENAI_API_KEY=your-api-key
OPENAI_MODEL=your-model-name

# Embedding Configuration
EMBEDDING_API_KEY=your-embedding-api-key
EMBEDDING_BASE_URL=your-embedding-base-url
EMBEDDING_MODEL=your-embedding-model

# Security
BCRYPT_ROUNDS=10

# Qwen OAuth
QWEN_CLIENT_ID=your-qwen-client-id
QWEN_CLIENT_SECRET=your-qwen-client-secret

# CORS
CORS_ORIGIN=http://localhost:5173
