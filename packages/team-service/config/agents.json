{
  "discovery": {
    "method": "config",
    "fallbackEnabled": false,
    "healthCheckInterval": 30000,
    "connectionTimeout": 5000
  },
  "agents": [
    {
      "id": "qwen-core-primary",
      "endpoint": "ws://localhost:8001",
      "healthCheck": "http://localhost:8001/health",
      "capabilities": ["session.create", "chat.send", "tools.execute", "session.destroy", "session.getStats", "session.updateTokens"],
      "priority": 1,
      "maxSessions": 100,
      "metadata": {
        "name": "Qwen Core Agent",
        "version": "1.0.0",
        "region": "local"
      }
    }
  ]
}
